{
	"name": "synapse-language-support",
	"displayName": "Synapse Language Support",
	"description": "This is an XML based Synapse Language Support extension for VSCode",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:xml",
		"onCommand:extension.start"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "SynapseXml",
				"extensions": [
					
				]
			}
		],
		"grammars": [
			{
				"language": "SynapseXml",
				"scopeName": "synapse.xml",
				"path": "./syntaxes/xml.tmLanguage.json"
			}
		],
		"commands": [
		
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"fast-xml-parser": "^3.12.12",
		"he": "^1.2.0",
		"xmldom": "^0.1.27"
	}
}
